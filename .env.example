APP_NAME="Border Buyers"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-domain.com

LOG_CHANNEL=stderr
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=border_buyers
DB_USERNAME=border_buyer_user
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true
SESSION_HTTP_ONLY=true

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_URL=

MAIL_MAILER=smtp
MAIL_HOST=smtp.sendgrid.net
MAIL_PORT=587
MAIL_USERNAME=apikey
MAIL_PASSWORD=your-sendgrid-api-key
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@your-domain.com
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=border-buyers-storage
AWS_USE_PATH_STYLE_ENDPOINT=false

# Cache Configuration
CACHE_TTL_LISTINGS=3600
CACHE_TTL_AGENTS=7200
CACHE_TTL_DASHBOARD=1800
CACHE_TTL_SEARCH=900
CACHE_TTL_DIRECTORY=3600
CACHE_TTL_DEFAULT=1800
CACHE_PREFIX=border_buyers_
CACHE_FALLBACK_ENABLED=true
CACHE_COMPRESSION_ENABLED=false

# Security Configuration
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,your-domain.com
SESSION_DOMAIN=.your-domain.com

# Performance Configuration
QUEUE_CONNECTION=redis
REDIS_CACHE_DB=1

# Render.com Specific Configuration
RENDER_EXTERNAL_URL=
RENDER_EXTERNAL_HOSTNAME=

# AI Services Configuration (Choose one: dummy, openai, anthropic, google)
AI_SERVICE=dummy

# OpenAI Configuration (Optional - for advanced AI analysis)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=500
OPENAI_TEMPERATURE=0.3

# Anthropic Claude Configuration (Optional - alternative to OpenAI)
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_MAX_TOKENS=500
ANTHROPIC_TEMPERATURE=0.3

# Google AI Configuration (Optional - alternative to OpenAI/Anthropic)
GOOGLE_AI_API_KEY=
GOOGLE_AI_MODEL=gemini-pro
GOOGLE_AI_MAX_TOKENS=500
GOOGLE_AI_TEMPERATURE=0.3

# AI Service Settings
AI_FALLBACK_TO_DUMMY=true
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600
AI_RATE_LIMIT_ENABLED=true
AI_RATE_LIMIT_ATTEMPTS=10
AI_RATE_LIMIT_MINUTES=1

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
